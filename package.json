    "mocha": "^5.2.0",
    "node-sass": "^4.13.1",
    "nodemon": "^2.0.2",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "supertest": "^3.1.0",
    "webpack": "^4.6.0",
    "webpack-cli": "^3.1.2"
  },
  "dependencies": {
